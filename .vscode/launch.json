{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch TUI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/tui/main.go",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Launch Findex",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/findex/main.go",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Launch CLI Findex",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/cli/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["-q=first"]
        },
        {
            "name": "Launch WEB Findex",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/webserver/main.go",
            "cwd": "${workspaceFolder}"
        }
    ]
}